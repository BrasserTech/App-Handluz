{
  "name": "app-handluz",
  "version": "1.0.0",
  "private": true,
  "main": "expo/AppEntry",
  "scripts": {
    "start": "npm exec -- expo start",
    "android": "npm exec -- expo run:android",
    "ios": "npm exec -- expo run:ios",
    "web": "npm exec -- expo start --web",
    "web:build": "npm exec -- expo export --platform web && node scripts/setup-pwa.js",
    "web:serve": "npx serve dist",
    "web:server": "node server/webServer.js",
    "web:build:serve": "node scripts/build-and-serve.js",
    "web:cloudflared": "powershell -ExecutionPolicy Bypass -File .\\iniciar-com-cloudflared.ps1",
    "deploy:vercel": "npm run web:build && cd dist && vercel --yes",
    "deploy:netlify": "npm run web:build && netlify deploy --prod --dir=dist",
    "lint": "npm exec -- expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/drawer": "^7.7.3",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.6.4",
    "@supabase/supabase-js": "^2.84.0",
    "axios": "^1.7.9",
    "expo": "~54.0.25",
    "expo-constants": "~18.0.11",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.9",
    "expo-notifications": "~0.32.14",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "express": "^5.1.0",
    "puppeteer": "^24.32.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "eas-cli": "^16.28.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  }
}
